package Scripting;

import java.util.ArrayList;

public class Prac {
	
	public static void main(String[]args) {
		
		int[] arrayList = {13,2,4,5,6,17};
		int temp = 0;
		temp = arrayList[0];
		arrayList[0]=arrayList[5];
		arrayList[5]=temp;
		System.out.println(arrayList[0]);
		System.out.println(arrayList[5]);
			
		}
		
		
	}
	

}
